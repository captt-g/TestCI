{
    "branches": [
        "+([0-9])?(.{+([0-9]),x}).x",
        "master",
        {
            "name": "develop",
            "prerelease": true
        }
    ],
    "dryRun": false,
    "debug": false,
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        "@semantic-release/github"
    ],
    "preset": "angular"
}